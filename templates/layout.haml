!!!
!= '<meta http-equiv="Content-Type" content="text/html;charset=UTF-8">'
%html
  %head
    %link(rel="stylesheet" href="/app.css" media="all")
    %script{src: "/socket.io/socket.io.js"}
    %script{src: "/transitive.js"}
    %script{src: "/templates.js"}
    :script
      Transitive = require("./transitive");
      Transitive.Views.templates = require("./templates.js");
      jQuery = $;
      if(window.console){}else{
        console = {};
        console.log = function(){}
      }
    %script{src: "/jquery.flot.js"}
    %script{src: "/jquery.flot.selection.js"}
    != "<!--[if lt IE 9]>"
    != '<script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>'
    != "<![endif]-->"

    %title
     dX-dT.com Change over time
  %body
    %div( class="topbar")
      %div( class="fill")
        %div( class="container")
          %h3
            %a(href="#" style="letter-spacing:3px;")
              dx-dt
          %ul
            %li
              %a(href="/") Home
            :if this.crumbs
              :each idx in this.crumbs
                %li
                  %a.skinny
                    >
                %li
                  %a(href="#masthead")= idx
          %ul.secondary-nav
            %li
              %form
                %input.search
            %li.dropdown(style="margin-left:10px")
              %a.dropdown-toggle(href="#toggle") Account
              %ul.dropdown-menu
                %li
                  %a(href="#") secondary-link
                  
              
    %div(style="margin-top:40px")
    .container
      != content
    :script
      !{etc}
      Transitive.boot();

