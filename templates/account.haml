.container-fluid
  .sidebar
    %h3 Environments
    .listing
      >
      %a(href="/")> Dev
    %h3 Servers
    .listing
      >
      %a(href="/")> e3983jfjn98n
    %h3 Processes
    %table
      - for(pid in status.processes.args){
      %tr>
        %td(style="text-align:right")= pid
        %td
          %b>= status.processes.ucomm[pid]
      - }
  .content
    %table.zebra-striped
      %thead
        %tr
          %th PID
          - columns = "lstart ucomm mem cpu rss vsz utime time".split(" ")
          - for(idx in columns){
          %th= columns[idx].toUpperCase()
          - }
      %tbody
        - for(pid in status.processes.lstart){
          status.processes.lstart[pid] = new Date(Date.parse(status.processes.lstart[pid])).toDateString()
        %tr
          %td= pid
          - for(idx in columns){
          %td
            - name = columns[idx]
            = (status.processes[name] && status.processes[name][pid])
          - }
        - }